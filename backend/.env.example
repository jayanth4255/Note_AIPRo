# Backend Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/notesaipro
SECRET_KEY=your-secret-key-min-32-characters-long-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# OpenAI Configuration (supports any OpenAI-compatible endpoint)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4

# File Upload Settings
MAX_FILE_SIZE_MB=50
ALLOWED_FILE_TYPES=pdf,doc,docx,txt,png,jpg,jpeg,gif,mp3,mp4,wav,mov

# CORS Settings
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379/0

# App Settings
DEBUG=True
APP_NAME=NoteAI Pro
